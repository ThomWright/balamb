{
  "name": "balamb",
  "version": "0.0.1",
  "private": true,
  "description": "Seeding",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "test-bin": "./src/bin/test.ts"
  },
  "scripts": {
    "check-ts-types": "tsc --noEmit",
    "lint": "eslint --ext js,ts src/ test/",
    "check-formatting": "./node_modules/.bin/prettier '**/*.ts' --list-different",
    "fix-formatting-all": "./node_modules/.bin/prettier '**/*.ts' --write",
    "fix-formatting": "./node_modules/.bin/prettier $(git status --porcelain | sed 's/^...//' | grep -E '\\.tsx?$') --write",
    "build": "rm -rf ./dist && rsync -a --exclude='*.ts' --exclude='__tests__' --prune-empty-dirs src/ dist/ && tsc --project tsconfig-build.json",
    "start": "ts-node ./src/index.ts"
  },
  "husky": {
    "hooks": {
      "commit-msg": "node ./node_modules/fit-commit-js/lib/hook.js .git/COMMIT_EDITMSG",
      "pre-commit": "npm run check-formatting && npm run lint",
      "pre-push": "npm run lint && npm run test && npm run check-licenses -- --summary"
    }
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "type-fest": "^0.20.2"
  },
  "devDependencies": {
    "@candide/eslint-config-node-service": "^2.0.0",
    "@types/node": "^12.19.7",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "eslint": "^7.19.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "fit-commit-js": "^0.3.2",
    "husky": "^4.3.8",
    "prettier": "^2.2.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  }
}
